<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">com.ibm.db2.jdbc.DB2Driver</property>
        <property name="hibernate.connection.password">db2@admin</property>
        <property name="hibernate.connection.url">jdbc:db2://localhost:50000/test</property>
        <property name="hibernate.connection.username">bfdbusr</property>
        <property name="hibernate.default_schema">CBSAUDIT</property>
        <property name="hibernate.dialect">org.hibernate.dialect.DB2Dialect</property>
    </session-factory>
</hibernate-configuration>
